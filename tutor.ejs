    <style>
        .invisible {
            display: none;
        }

        .inner-banner-override {
            background: url(/image.jpg) no-repeat center;
            background-size: cover;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            -ms-background-size: cover;
            min-height: 300px;
            position: relative;
            z-index: 1;
        }

        .inner-banner-override::before {
            content: "";
            position: absolute;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.4);
            z-index: -1;
        }

        .mydropdown {
            display: none;
            width: 100%;
            padding: 0.5rem 1rem;
            border: none;
            color: #ffffff;
        }

        .pane-menu {
            display: grid;
            grid-template-columns: repeat(7, 1fr);
        }

        @media (max-width: 990px) {
            .pane-menu {
                display: none;
            }

            .mydropdown {
                display: block;
            }
        }
    </style>
</head>

